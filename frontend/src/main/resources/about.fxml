<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<BorderPane id="pane" stylesheets="styleSheets/about.css" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="220.0" prefWidth="300.0"
            xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ch.supsi.connectfour.frontend.view.AboutView">
    <top>
        <Label fx:id="aboutConnectFourLabel" text="About ConnectFour" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets top="10.0"/>
            </BorderPane.margin>
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
    </top>
    <center>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0" BorderPane.alignment="CENTER">
            <children>
                <ImageView fx:id="imageView" fitHeight="90.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true"/>
                <VBox prefHeight="153.0" prefWidth="168.0" spacing="5.0">
                    <children>
                        <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
                            <children>
                                <HBox prefHeight="100.0" prefWidth="200.0" spacing="3.0">
                                    <children>
                                        <Label fx:id="builtOnLabel" text="Built on ">
                                            <font>
                                                <Font size="12.0"/>
                                            </font>
                                        </Label>
                                        <Label text="May 20 2024">
                                            <font>
                                                <Font size="12.0"/>
                                            </font>
                                        </Label>
                                    </children>
                                </HBox>
                                <HBox prefHeight="100.0" prefWidth="200.0" spacing="3.0">
                                    <children>
                                        <Label fx:id="runtimeVersionLabel" text="Runtime version "/>
                                        <Label text="1.0.0">
                                            <font>
                                                <Font size="12.0"/>
                                            </font>
                                        </Label>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                            <children>
                                <Label fx:id="poweredByLabel" text="Powered by">
                                    <font>
                                        <Font size="12.0"/>
                                    </font>
                                </Label>
                                <Label text="Martina Galasso">
                                    <font>
                                        <Font size="12.0"/>
                                    </font>
                                </Label>
                                <Label text="Alex Rodrigues Costa">
                                    <font>
                                        <Font size="12.0"/>
                                    </font>
                                </Label>
                            </children>
                        </VBox>
                    </children>
                </VBox>
            </children>
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
        </HBox>
    </center>
    <bottom>
        <Button fx:id="button" mnemonicParsing="false" text="Close" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="10.0" top="5.0"/>
            </BorderPane.margin>
        </Button>
    </bottom>
</BorderPane>
